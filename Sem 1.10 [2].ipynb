    "def puk_func(n):\n",
    "    puk = \"пук\"\n",
    "    tire = \"-\"\n",
    "    if n == 0: \n",
    "        return \"\" \n",
    "    if n < 0:\n",
    "        puk = puk[-1::-1]\n",
    "    \n",
    "    return (puk + tire) * (abs(n)-1) + puk"
   ]
